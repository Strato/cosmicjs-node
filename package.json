		"nyc": "^15.1.0",
		"rewire": "^6.0.0",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.10.0"
	},
	"engines": {
		"node": ">=10.0"
